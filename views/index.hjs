<!DOCTYPE html>
<html>
  <head>
    <title>Node Wiki</title>
    <link rel='stylesheet' href='/css/css.css' />
    <script type="text/javascript" src="js/lib/angular.min.js"></script>
    <script type="text/javascript" src="js/lib/angular-route.min.js"></script>
    <script type="text/javascript" src="js/app.js"></script>
  </head>
    <body ng-app="nodeWiki">
    <noscript>Sorry you need javascript for this application to work</noscript>

    <div id="sidebar" ng-controller="navCtrl">
      <h1>NODE WIKI</h1>

      <div id="pages-list">
        <file-item ng-repeat="item in files | orderBy: 'name'" item="item" open="openItem(itemName)">
        </file-item>
      </div>


    </div><!-- sidebar -->

    <div id="wiki-content" ng-view>
      <p>{{ hey }}</p>

    </div><!-- wiki-content -->


  </body>
</html>
